<div class="col-md-4">
  <div class="card" data-id="<%= portfolio_item.id %>">
    <%= image_tag portfolio_img(portfolio_item,'thumb'), style: 'width: 100%' %>
    <p class="card-text">
      <span><%= link_to portfolio_item.title, portfolio_item %></span> <%= portfolio_item.subtitle %>
    </p>
  </div>
</div>